<Button>    size: 100, 80<DumCulatorWidget>    BoxLayout:        orientation: "vertical"        size: root.width, root.height        background_normal: ''        background_color: (26/255, 26/255, 26/255, 1)        TextInput:            id: input_box            text: "0"            size_hint: (1, 0.25)            halign: "right"            font_size: 40            background_color: (1, 1, 1, 1)            background_normal: ''        GridLayout:            cols: 4            rows: 3            spacing: 10, 10            padding: 10, 10            ### row 1 ###            RoundedButton:                text: "7"                font_size: 32                on_press: root.button_value(7)            RoundedButton:                text: "8"                font_size: 32                on_press: root.button_value(8)            RoundedButton:                text: "9"                font_size: 32                on_press: root.button_value(9)            RoundedButton:                text: "0"                font_size: 32                on_press: root.button_value(0)            ### row 2 ###            RoundedButton:                text: "4"                font_size: 32                on_press: root.button_value(4)            RoundedButton:                text: "5"                font_size: 32                on_press: root.button_value(5)            RoundedButton:                text: "6"                font_size: 32                on_press: root.button_value(6)            RoundedButton:                text: "-"                font_size: 32                on_press: root.get_sign("-")            ### row 3 ###            RoundedButton:                text: "1"                font_size: 32                on_press: root.button_value(1)            RoundedButton:                text: "2"                font_size: 32                on_press: root.button_value(2)            RoundedButton:                text: "3"                font_size: 32                on_press: root.button_value(3)            RoundColorButton:                text: "="                font_size: 32<RoundedButton@Button>    background_normal: ''    background_color: (0, 0, 0, 0)    canvas.before:        Color:            rgba: (35/255, 35/255, 35/255, 1)        RoundedRectangle:            size: self.size            pos: self.pos            radius: [15]<RoundColorButton@Button>    background_normal: ''    background_color: (1, 1, 1, 0)    canvas.before:        Color:            rgba: (219/255, 165/255, 37/255, 1)        RoundedRectangle:            size: self.size            pos: self.pos            radius: [15]